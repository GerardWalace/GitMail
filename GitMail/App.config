<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section
      name="gitMailConfiguration"
      type="GitMail.Configuration.GitMailConfiguration, GitMail" />
  </configSections>
  <gitMailConfiguration>
    <repositories>
      <repositoryConfiguration
        repositoryPath="http://repotestpath"
        directoryPath="test">
        <merges>
          <mergeConfiguration
            mailObject="V1P5 to master"
            intoBranch="master"
            fromBranch="V1P5"
            mailsDesReferents="test@test.com" />
          <mergeConfiguration
            mailObject="RealisationOperation to master"
            intoBranch="master"
            fromBranch="RealisationOperation"
            mailsDesReferents="test@test.com" />
        </merges>
      </repositoryConfiguration>
    </repositories>
  </gitMailConfiguration>
  <startup>
    <supportedRuntime
      version="v4.0"
      sku=".NETFramework,Version=v4.5" />
  </startup>
</configuration>
