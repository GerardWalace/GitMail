<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section
      name="gitMailConfiguration"
      type="GitMail.Configuration.GitMailConfiguration, GitMail" />
  </configSections>
  <gitMailConfiguration>
    <repositories>
      <repositoryConfiguration
        repositoryPath="http://repotestpath"
        directoryPath="test"
        mail_Host="smtp.gmail.com"
        mail_Port="25"
        mail_EnableSsl="True"
        mail_From="test.git@gmail.com"
        mail_Login="test.git@gmail.com"
        mail_Password="password">
        <merges>
          <mergeConfiguration
            mailObject="[GitMail] Simulation de merge V1P6.1-* vers V1P6.1"
            intoBranch="origin/V1P6.1"
            fromBranch="origin/V1P6.1-"
            isMultiBranch="true"
            mailsDesReferents="test@test.com" />
        </merges>
      </repositoryConfiguration>
    </repositories>
  </gitMailConfiguration>
  <startup>
    <supportedRuntime
      version="v4.0"
      sku=".NETFramework,Version=v4.5" />
  </startup>
</configuration>
