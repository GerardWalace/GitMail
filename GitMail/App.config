<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section
      name="gitMailConfiguration"
      type="GitMail.Configuration.GitMailConfiguration, GitMail" />
  </configSections>
  <gitMailConfiguration>
    <repositories>
      <repositoryConfiguration
        repositoryPath="http://repotestpath"
        directoryPath="test">
        <merges>
          <mergeConfiguration
            mailObject="[GitMail] Simulation de merge V1P5 vers master"
            intoBranch="origin/master"
            fromBranch="origin/V1P5"
            mailsDesReferents="test@test.com" />
          <mergeConfiguration
            mailObject="[GitMail] Simulation de merge RealisationOperation vers master"
            intoBranch="origin/master"
            fromBranch="origin/RealisationOperation"
            mailsDesReferents="test@test.com" />
          <mergeConfiguration
            mailObject="[GitMail] Simulation de merge ReferencesInternes vers master"
            intoBranch="origin/master"
            fromBranch="origin/ReferencesInternes"
            mailsDesReferents="test@test.com" />
        </merges>
      </repositoryConfiguration>
    </repositories>
  </gitMailConfiguration>
  <startup>
    <supportedRuntime
      version="v4.0"
      sku=".NETFramework,Version=v4.5" />
  </startup>
</configuration>
